{
  "_version": "1.40.0",
  "sap.app": {
    "id": "com.karafi.karafisapui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.7.1",
      "toolsId": "2222123d-fab0-43bb-b77d-058f03063f8a"
    },
    "dataSources": {
      "env": {
        "uri": "model/api.json",
        "type": "http",
        "settings": {}
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.106.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.karafi.karafisapui5.i18n.i18n"
        }
      },
      "env": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/env.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.karafi.karafisapui5.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "HomeRoute",
          "pattern": "",
          "target": [
            "HomeTarget"
          ]
        },
        {
          "name": "UserRoute",
          "pattern": "manageUsers",
          "target": [
            "userTarget"
          ]
        }
      ],
      "targets": {
        "HomeTarget": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "home",
          "viewName": "home"
        },
        "userTarget": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "user",
          "viewName": "user"
        }
      }
    },
    "rootView": {
      "viewName": "com.karafi.karafisapui5.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}